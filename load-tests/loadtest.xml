<?xml version="1.0"?>
<!DOCTYPE tsung SYSTEM "/home/ec2-user/opt/tsung-1.5.0/share/tsung/tsung-1.0.dtd">
<tsung loglevel="notice" version="1.0">

  <!-- Client side setup -->
  <clients>
    <client host="localhost" use_controller_vm="true" maxusers='15000'/>
  </clients>

  <!-- Server side setup -->
  <!-- IMPORTANT: CHANGE HOST TO YOUR TSUNG HOST -->
  <servers>
    <server host="ec2-52-33-250-16.us-west-2.compute.amazonaws.com" port="80" type="tcp"></server>
  </servers>


  <load>
    <arrivalphase phase="1" duration="30" unit="second">
      <users arrivalrate="1" unit="second"></users>
    </arrivalphase>
    <arrivalphase phase="2" duration="30" unit="second">
      <users arrivalrate="2" unit="second"></users>
    </arrivalphase>
    <arrivalphase phase="3" duration="30" unit="second">
      <users arrivalrate="4" unit="second"></users>
    </arrivalphase>
<!--
   <arrivalphase phase="4" duration="30" unit="second">
     <users arrivalrate="8" unit="second"></users>
   </arrivalphase>
   <arrivalphase phase="5" duration="30" unit="second">
     <users arrivalrate="16" unit="second"></users>
   </arrivalphase>
   <arrivalphase phase="6" duration="30" unit="second">
     <users arrivalrate="32" unit="second"></users>
   </arrivalphase>
   <arrivalphase phase="7" duration="30" unit="second">
     <users arrivalrate="64" unit="second"></users>
   </arrivalphase>
   <arrivalphase phase="8" duration="30" unit="second">
     <users arrivalrate="128" unit="second"></users>
   </arrivalphase>
   <arrivalphase phase="9" duration="30" unit="second">
     <users arrivalrate="256" unit="second"></users>
   </arrivalphase>
   <arrivalphase phase="10" duration="30" unit="second">
     <users arrivalrate="512" unit="second"></users>
   </arrivalphase>
-->
  </load>

  <options>
    <!-- Set connection timeout to 2 seconds -->
    <option name="global_ack_timeout" value="2000"></option>

    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>

  <!-- ================================================= -->
  <!--                     Sessions Weight               -->
  <!-- [user_signup]                  3                  -->
  <!-- [user_signin]                  40                 -->
  <!-- [user_createcourse]            1                  -->
  <!-- [user_joincourse]              30                 -->
  <!-- [user_creatteam]               15                 -->
  <!-- [invite]                       10                 -->
  <!-- ================================================= -->

  <sessions>
    <!-- ================================================= -->
    <!--                      SESSION                      -->
    <!--                    user_signup                    -->
    <!-- ================================================= -->
    <!-- 1. signup -->
    <!-- 2. log out -->
    <!-- ================================================= -->

    <session name="user_signup" probability="3" type="ts_http">

      <request>
        <http url='/' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request>
        <http url='/users/sign_up' version='1.1' method='GET'></http>
      </request>

      <setdynvars sourcetype="eval"
            code='fun({Pid,DynVars})-> [[13, 10]] end.'>
        <var name="nl" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="9999999">
        <var name="random_user_number" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="9999999">
        <var name="random_boundary_number" />
      </setdynvars>

      <request subst="true">
        <http
          url='/users'
          version='1.1'
          method='POST'
          content_type='multipart/form-data; boundary=----WebKitFormBoundary%%_random_boundary_number%%'
          contents='------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="user[email]"%%_nl%%
          %%_nl%%
          user%%_random_user_number%%@gmail.com%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="user[password]"%%_nl%%
          %%_nl%%
          11111111%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="user[password_confirmation]"%%_nl%%
          %%_nl%%
          11111111%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="user[first_name]"%%_nl%%
          %%_nl%%
          %%_random_user_number%%first%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="user[last_name]"%%_nl%%
          %%_nl%%
          %%_random_user_number%%last%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="user[github]"%%_nl%%
          %%_nl%%
          https://github.com/user%%_random_user_number%%%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%
          Content-Disposition: form-data; name="commit"%%_nl%%
          %%_nl%%
          Sign up%%_nl%%
          ------WebKitFormBoundary%%_random_boundary_number%%%%_nl%%%%_nl%%
          '
          >
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http url='/users/sign_out' version='1.1' method='GET'></http>
      </request>

    </session>

    <!-- ================================================= -->
    <!--                      SESSION                      -->
    <!--                    user_signin                    -->
    <!-- ================================================= -->
    <!-- 1. signin -->
    <!-- 2. update forfile -->
    <!-- 3. log out -->
    <!-- ================================================= -->

    <!-- User signin and update profile -->
    <session name="user_signin" probability="40" type="ts_http">

      <!-- start out at the dashboard. -->
      <request>
        <http url='/' version='1.1' method='GET'></http>
      </request>

      <!-- wait for up to 2 seconds, user is looking at posts -->
      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1000" end="9999999">
        <var name="random_user_id" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="100000000" end="999999999">
        <var name="random_user_id_update" />
      </setdynvars>

      <!-- signin -->
      <request subst="true">
        <http
          url='/users/sign_in'
          version='1.1'
          method='POST'
          contents='user%5Bemail%5D=user%%_random_user_id%%%40email.com&amp;
          user%5Bpassword%5D=password%%_random_user_id%%&amp;
          user%5Bremember_me%5D=0&amp;commit=Log+in'>
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- view notification -->
      <request>
        <http url='/notifications' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- view message -->
      <request>
        <http url='/mailbox/inbox' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- edit profile: get form-->
      <request>
        <http url='/users/edit' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- edit profile: send form-->
      <request subst="true">
        <http
          url='/users/edit'
          version='1.1'
          method='POST'
          contents='user%5Bemail%5D=useremail%%_random_user_id_update%%%40email.com&amp;
          user%5Bfirst_name%5D=userfirst%%_random_user_id_update%%&amp;
          user%5Blast_name%5D=userlast%%_random_user_id_update%%&amp;
          user%5Bgithub%5D=usergithub%%_random_user_id_update%%&amp;
          user%5Bsid%5D=%%_random_user_id_update%%&amp;
          user%5Bmajor%5D=usermajor%%_random_user_id_update%%&amp;
          user%5Bpassword%5D=userpassword%%_random_user_id_update%%&amp;
          user%5Bpassword_confirmation%5D=userpassword%%_random_user_id_update%%&amp;
          user%5Bcurrent_password%5D=userpassword%%_random_user_id%%&amp;
          commit=Update'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http url='/users/sign_out' version='1.1' method='GET'></http>
      </request>

    </session>

    <!-- ================================================= -->
    <!--                      SESSION                      -->
    <!--                    user_createcourse              -->
    <!-- ================================================= -->
    <!-- 1. signin -->
    <!-- 2. create course -->
    <!-- 3. log out -->
    <!-- ================================================= -->

    <session name="user_createcourse" probability="1" type="ts_http">
      <!-- start out at the dashboard. -->
      <request>
        <http url='/' version='1.1' method='GET'></http>
      </request>

      <!-- wait for up to 2 seconds, user is looking at posts -->
      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1000" end="9999999">
        <var name="random_user_id" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="10">
        <var name="random_team_size" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="20">
        <var name="random_time_commitment" />
      </setdynvars>

      <!-- signin -->
      <request subst="true">
        <http
          url='/users/sign_in'
          version='1.1'
          method='POST'
          contents='user%5Bemail%5D=user%%_random_user_id%%%40email.com&amp;
          user%5Bpassword%5D=password%%_random_user_id%%&amp;
          user%5Bremember_me%5D=0&amp;
          commit=Log+in'>
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http
          url='/courses'
          version='1.1'
          method='POST'
          contents='course%5Bname%5D=course%%_random_user_id%%&amp;
          course%5Bdescription%5D=coursedescription%%_random_user_id%%&amp;
          course%5Bmax_members%5D=%%_random_team_size%%&amp;
          course%5Bmin_time_commitment%5D=%%_random_time_commitment%%&amp;
          course%5Bstart_date%281i%29%5D=2015&amp;
          course%5Bstart_date%282i%29%5D=11&amp;
          course%5Bstart_date%283i%29%5D=13&amp;
          course%5Bend_date%281i%29%5D=2015&amp;
          course%5Bend_date%282i%29%5D=11&amp;
          course%5Bend_date%283i%29%5D=13&amp;
          commit=Create'>
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http url='/users/sign_out' version='1.1' method='GET'></http>
      </request>

    </session>

    <!-- ================================================= -->
    <!--                      SESSION                      -->
    <!--                    user_joincourse                -->
    <!-- ================================================= -->
    <!-- 1. signin -->
    <!-- 2. view course -->
    <!-- 3. join course -->
    <!-- 4. view course info -->
    <!-- 5. log out -->
    <!-- ================================================= -->

    <session name="user_joincourse" probability="30" type="ts_http">

      <!-- start out at the dashboard. -->
      <request>
        <http url='/' version='1.1' method='GET'></http>
      </request>

      <!-- wait for up to 2 seconds, user is looking at posts -->
      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1000" end="9999999">
        <var name="random_user_id" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="10">
        <var name="random_team_size" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="20">
        <var name="random_time_commitment" />
      </setdynvars>

      <!-- signin -->
      <request subst="true">
        <http
          url='/users/sign_in'
          version='1.1'
          method='POST'
          contents='user%5Bemail%5D=user%%_random_user_id%%%40email.com&amp;
          user%5Bpassword%5D=password%%_random_user_id%%&amp;
          user%5Bremember_me%5D=0&amp;
          commit=Log+in'>
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1" end="15">
        <var name="random_courseid" />
      </setdynvars>

      <!-- view all courses that can join-->
      <request>
        <http url='/join_classes/index' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- go to course page-->
      <request>
        <http url='/courses/%%_random_courseid%%' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- join course-->
      <request>
        <http url='/courses/%%_random_courseid%%/join' version='1.1' method='POST'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- view classmates -->
      <request>
        <http url='/courses/%%_random_courseid%%#classmates' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- sort teammate based on time -->
      <request>
        <http url='/courses/%%_random_courseid%%?order_skill=-1' version='1.1' method='GET'></http>
      </request>

      <!-- sort teammate based on overall rating -->
      <request>
        <http url='/courses/%%_random_courseid%%?order_skill=-2' version='1.1' method='GET'></http>
      </request>

      <!-- view teams -->
      <request>
        <http url='/courses/%%_random_courseid%%#teams' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http url='/users/sign_out' version='1.1' method='GET'></http>
      </request>

    </session>

    <!-- ================================================= -->
    <!--                      SESSION                      -->
    <!--                    user_createteam                -->
    <!-- ================================================= -->
    <!-- 1. signin -->
    <!-- 2. view course -->
    <!-- 3. join course -->
    <!-- 4. create team -->
    <!-- 5. log out -->
    <!-- ================================================= -->

    <session name="user_createteam" probability="15" type="ts_http">

      <!-- start out at the dashboard. -->
      <request>
        <http url='/' version='1.1' method='GET'></http>
      </request>

      <!-- wait for up to 2 seconds, user is looking at posts -->
      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1000" end="9999999">
        <var name="random_user_id" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="100000000" end="999999999">
        <var name="random_user_id_update" />
      </setdynvars>

      <!-- signin -->
      <request subst="true">
        <http
          url='/users/sign_in'
          version='1.1'
          method='POST'
          contents='user%5Bemail%5D=user%%_random_user_id%%%40email.com&amp;
          user%5Bpassword%5D=password%%_random_user_id%%&amp;
          user%5Bremember_me%5D=0&amp;commit=Log+in'>
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1" end="15">
        <var name="random_courseid" />
      </setdynvars>

      <!-- view all courses that can join-->
      <request>
        <http url='/join_classes/index' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- go to course page-->
      <request>
        <http url='/courses/%%_random_courseid%%' version='1.1' method='GET'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <!-- join course-->
      <request>
        <http url='/courses/%%_random_courseid%%/join' version='1.1' method='POST'></http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http
          url='/courses/%%_random_courseid%%/teams'
          version='1.1'
          method='POST'
          contents='team%5Bname%5D=team%%_random_courseid%%by%%_random_user_id%%&amp;
          team%5Bdescription%5D=course%%_random_courseid%%&amp;
          team%5Bpasscode%5D=0&amp;commit=Create+Team'>
        </http>
      </request>

      <thinktime value="2" random="true"></thinktime>

      <request subst="true">
        <http url='/users/sign_out' version='1.1' method='GET'></http>
      </request>

    </session>

    <!-- ================================================= -->
    <!--                      SESSION                      -->
    <!--                       invite                      -->
    <!-- ================================================= -->
    <!-- 1. invite -->
    <!-- ================================================= -->

    <session name="invite" probability="10" type="ts_http">

      <!-- start out at the dashboard. -->
      <request>
        <http url='/' version='1.1' method='GET'></http>
      </request>

      <!-- wait for up to 2 seconds, user is looking at posts -->
      <thinktime value="2" random="true"></thinktime>

      <setdynvars sourcetype="random_number" start="1" end="5">
        <var name="user_id" />
      </setdynvars>

      <setdynvars sourcetype="random_number" start="1" end="15">
        <var name="team_id" />
      </setdynvars>

      <thinktime value="2" random="true"></thinktime>

      <request>
         <http url='/users/invite?id=%%_user_id%%&amp;team_id=%_team_id%' version='1.1' method='POST'></http>
      </request>

    </session>

  </sessions>
</tsung>
